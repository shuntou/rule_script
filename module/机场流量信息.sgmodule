#!name=节点订阅URL信息
#!desc=显示节点订阅URL信息
#!category=面板
#!system=ios
#!arguments=url:将URL文本转换成Encode填入, reset_day:1, title:随便填, icon:bolt.horizontal.circle.fill, color:#007aff
[Panel]
Sub_info = script-name=Sub_info,update-interval=3600
[Script]
Sub_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js,script-update-interval=0,argument=url={{{url}}}&reset_day={{{reset_day}}}&title={{{title}}}&icon={{{icon}}}&color={{{color}}}
